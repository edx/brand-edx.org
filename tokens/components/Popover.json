{
  "color": {
    "popover": {
      "header": {
        "bg": { "value": "transparent", "source": "$popover-header-bg" }
      }
    }
  },
  "size": {
    "popover": {
      "border": {
        "width": { "value": "0", "source": "$popover-border-width" }
      }
    }
  },
  "elevation": {
    "popover": {
      "box-shadow": {
        "value": "{elevation.box-shadow.level.2.value}",
        "source": "$popover-box-shadow"
      }
    }
  }
}
