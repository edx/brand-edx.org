{
  "enable": {
    "caret": { "value": "true", "themeable": true, "source": "$enable-caret" },
    "rounded": { "value": "true", "themeable": true, "source": "$enable-rounded" },
    "shadows": { "value": "true", "themeable": true, "source": "$enable-shadows" },
    "gradients": { "value": "false", "themeable": true, "source": "$enable-gradients" },
    "transitions": { "value": "false", "themeable": true, "source": "$enable-transitions" },
    "media": {
      "query": {
        "prefers": {
          "reduced": {
            "motion": { "value": "true", "themeable": true, "source": "$enable-prefers-reduced-motion-media-query" }
          }
        },
        "hover": { "value": "false", "themeable": true, "source": "$enable-hover-media-query" }
      }
    },
    "grid": {
      "classes": { "value": "true", "themeable": true, "source": "$enable-grid-classes" }
    },
    "pointer": {
      "cursor": {
        "for": {
          "buttons": { "value": "true", "themeable": true, "source": "$enable-pointer-cursor-for-buttons" }
        }
      }
    },
    "print": {
      "styles": { "value": "true", "themeable": true, "source": "$enable-print-styles" }
    },
    "font": {
      "size": {
        "responsive": { "value": "false", "themeable": true, "source": "$enable-responsive-font-sizes" }
      }
    },
    "validation": {
      "icons": { "value": "false", "themeable": true, "source": "$enable-validation-icons" }
    },
    "deprecation": {
      "messages": { "value": "true", "themeable": true, "source": "$enable-deprecation-messages" }
    }
  }
}
