{
  "caret": {
    "width": { "value": ".3em", "themeable": true, "source": "$caret-width" },
    "spacing": { "value": "{caret.width.value} * 1", "themeable": true, "source": "$caret-spacing" },
    "vertical": {
      "align": { "value": "{caret.width.value} * .67", "themeable": true, "source": "$caret-vertical-align" }
    }
  }
}
