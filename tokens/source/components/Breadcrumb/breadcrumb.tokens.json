{
  "breadcrumb": {
    "font": {
      "size": { "value": "null", "themeable": true, "source": "$breadcrumb-font-size" }
    },
    "padding": {
      "y": { "value": ".75rem", "themeable": true, "source": "$breadcrumb-padding-y" },
      "x": { "value": "1rem", "themeable": true, "source": "$breadcrumb-padding-x" },
      "item": { "value": ".5rem", "themeable": true, "source": "$breadcrumb-item-padding" }
    },
    "margin": {
      "bottom": { "value": "1rem", "themeable": true, "source": "$breadcrumb-margin-bottom" }
    },
    "border": {
      "radius": {
        "base": { "value": "{border.radius.base.value}", "themeable": true, "source": "$breadcrumb-border-radius" }
      }
    },
    "bg": { "value": "{color.gray.200.value}", "themeable": true, "source": "$breadcrumb-bg" },
    "color": {
      "divider": { "value": "{color.gray.600.value}", "themeable": true, "source": "$breadcrumb-divider-color" },
      "active": { "value": "{color.gray.500.value}", "themeable": true, "source": "$breadcrumb-active-color" }
    }
  }
}
