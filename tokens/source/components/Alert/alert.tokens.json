{
  "alert": {
    "padding": {
      "y": { "value": "1.5rem", "themeable": true, "source": "$alert-padding-y" },
      "x": { "value": "1.5rem", "themeable": true, "source": "$alert-padding-x" }
    },
    "margin": {
      "bottom": { "value": "1rem", "themeable": true, "source": "$alert-margin-bottom" }
    },
    "border": {
      "radius": { "value": "{border.radius.base}", "themeable": true, "source": "$alert-border-radius" },
      "width": { "value": "0", "themeable": true, "source": "$alert-border-width" }
    },
    "font": {
      "weight": {
        "link": { "value": "{font.weight.bold.value}", "themeable": true, "source": "$alert-link-font-weight" }
      }
    },
    "level": {
      "bg": { "value": "-10", "themeable": true, "source": "$alert-bg-level" },
      "border": { "value": "-9", "themeable": true, "source": "$alert-border-level" },
      "color": { "value": "6", "themeable": true, "source": "$alert-color-level" }
    }
  }
}
