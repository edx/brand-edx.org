{
  "dropdown": {
    "min": {
      "width": { "value": "10rem", "themeable": true, "source": "$dropdown-min-width" }
    },
    "padding": {
      "x": {
        "base": { "value": "0", "themeable": true, "source": "$dropdown-padding-x" },
        "item": { "value": "1.5rem", "themeable": true, "source": "$dropdown-item-padding-x" }
      },
      "y": {
        "base": { "value": ".5rem", "themeable": true, "source": "$dropdown-padding-y" },
        "item": { "value": ".625rem", "themeable": true, "source": "$dropdown-item-padding-y" }
      },
      "header": {
        "value": "{dropdown.padding.y.base.value} {dropdown.padding.x.item.value}",
        "themeable": true,
        "source": "$dropdown-header-padding"
      }
    },
    "spacer": { "value": ".125rem", "themeable": true, "source": "$dropdown-spacer" },
    "font": {
      "size": { "value": "{font.size.base.value}", "themeable": true, "source": "$dropdown-font-size" }
    },
    "color": {
      "base": { "value": "{body.color.value}", "themeable": true, "source": "$dropdown-color" },
      "header": { "value": "{color.gray.600.value}", "themeable": true, "source": "$dropdown-header-color" }
    },
    "bg": { "value": "{color.white.value}", "themeable": true, "source": "$dropdown-bg" },
    "border": {
      "color": { "value": "rgba({color.black.value}, .15)", "themeable": true, "source": "$dropdown-border-color" },
      "width": { "value": "0", "themeable": true, "source": "$dropdown-border-width" },
      "radius": { "value": "{border.radius.base.value}", "themeable": true, "source": "$dropdown-border-radius" }
    },
    "divider": {
      "bg": { "value": "{color.gray.200.value}", "themeable": true, "source": "$dropdown-divider-bg" },
      "margin": {
        "y": { "value": "{nav.divider.margin.y.value}", "themeable": true, "source": "$dropdown-divider-margin-y" }
      }
    },
    "box": {
      "shadow": { "value": "{box-shadow.level.2.value}", "themeable": true, "source": "$dropdown-box-shadow" }
    },
    "link": {
      "color": { "value": "{color.gray.900.value}", "themeable": true, "source": "$dropdown-link-color" },
      "hover": {
        "color": { "value": "{color.gray.900.value}", "themeable": true, "source": "$dropdown-link-hover-color" },
        "bg": { "value": "{color.gray.100.value}", "themeable": true, "source": "$dropdown-link-hover-bg" }
      },
      "active": {
        "color": {
          "value": "{core.component.active.color.value}", "themeable": true, "source": "$dropdown-link-active-color"
        },
        "bg": {
          "value": "{core.component.active.color.value}", "themeable": true, "source": "$dropdown-link-active-bg" }
      },
      "disabled": {
        "color": { "value": "{color.gray.600.value}", "themeable": true, "source": "$dropdown-link-disabled-color" }
      }
    }
  }
}
